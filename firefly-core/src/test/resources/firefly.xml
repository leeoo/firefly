<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://code.google.com/p/firefly/beans" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://code.google.com/p/firefly/beans http://firefly.googlecode.com/files/beans.xsd">
	<bean id="person" class="test.component3.Person">
		<property name="name" value="Jack"/>
		<property name="age" value="12"/>
	</bean>
	<bean id="person2" class="test.component3.Person">
		<property name="name">
			<value>Bob</value>
		</property>
		<property name="age" value="32"/>
	</bean>
	<bean id="personService" class="test.component3.PersonService">
		<property name="person" ref="person"/>
		<property name="testList">
			<list>
				<value type="int">123</value>
				<ref bean="person2"/>
				<value type="java.lang.String">dfdf</value>
				<ref bean="person"/>
			</list>
		</property>
		<property name="testList2">
			<list>
				<value type="java.lang.String">a1</value>
				<value type="java.lang.String">a2</value>
			</list>
		</property>
	</bean>
	
	<bean id="collectionService" class="test.component3.CollectionService">
		<property name="list">
			<list>
				<value>s1</value>
				<value>s2</value>
			</list>
		</property>
	</bean>
	
	<bean id="collectionService2" class="test.component3.CollectionService">
		<property name="list">
			<list>
				<value>s1</value>
				<value>s2</value>
				<list>
					<value>s3</value>
					<value>s4</value>
				</list>
			</list>
		</property>
	</bean>
</beans>
